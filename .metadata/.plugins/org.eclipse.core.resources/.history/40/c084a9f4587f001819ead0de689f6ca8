package com.javalec.ex;

public class DIExP {
	DIEx diex;
	
	public void nameP() {
		System.out.println(diex.getE_name());
		
	}
	
	public void numberP() {
		System.out.println(diex.getE_number());
		
	}
	
	public void emailP(String emailP)
	{
		System.out.println(diex.getE_email());
		
	}
	
	public void departmentP(int departmnetP) {
		
		System.out.println(diex.getE_department());
		
	}
	

}
